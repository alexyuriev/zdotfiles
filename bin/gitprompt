#!/bin/bash

k=`git config --get remote.origin.url 2> /dev/null`
repo=''
branch=''

if [ ! -z $k ]; then
  repo=`basename -s .git $k`


  if [ ! -z $repo ]; then
    branch=`git rev-parse --abbrev-ref HEAD`
  fi

  echo "git [$repo:\"$branch\"]"
fi

#echo "k=$k"
#echo "repo=$repo"
#echo "branch=$branch"