# .bash_aliases
#
# useful bash(1) aliaes

alias st="sublime_text"
alias ..="cd .."
alias gs="git status"
alias ga="git add -A"
alias gu="git add -u"
alias gcm="git commit -m "
alias gp="git push"
alias prep-catt="source ~/python-environments/catt/bin/activate"
alias ffmpeg.old="/usr/bin/ffmpeg"
alias ffmpeg="/opt/ffmpeg-4.1.1-amd64-static/ffmpeg"
alias ffprobe="/opt/ffmpeg-4.1.1-amd64-static/ffprobe"
